<tr class="col_defs">
[% IF sel_col %]<th width="1%">&nbsp;</th>[% END %]
[% FOREACH col_def IN col_defs; NEXT UNLESS col_def.visible %]
<th [% IF col_def.width %]width="[% col_def.width %]"[% END %]>
  <a href="[% col_def.href_sort %]" style="color: black">[% col_def.caption -%]
[% IF col_def.sort_up %]&nbsp;&#x25B2;[% END -%]
[% IF col_def.sort_down %]&nbsp;&#x25BC;[% END %]</a>
</th>
[% END %]
</tr>
